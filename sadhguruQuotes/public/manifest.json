{
  "name": "Daily Sadhguru Quotes",
  "description" : "Remind yourselves of the deep wisdom provided by Sadhguru on every new tab",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "action": {
    "default_icon": "logo192.png"
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
}